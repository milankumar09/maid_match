        /* Add CSS for sliding banner */
        .sliding-banner {
            width: 100%;
            height: 700px;
            background-repeat: no-repeat;
            background-size: cover;
            animation: slide 5s infinite;
        }

        @keyframes slide {
            0%, 25% { background-image: url('/image/1.jpg'); }
            25%,50% { background-image: url('/image/2.jpg'); }
            50%,75% { background-image: url('/image/3.jpg'); }
            75%, 100% { background-image: url('/image/4.jpg'); }
        }

        /* Change text color */
        .highlighted-text {
            color: #ff6600; /* Orange color */
            background-color: #fff8dc; /* Pale yellow background */
            padding: 2px 5px; /* Add some padding for better readability */
            border-radius: 3px; /* Rounded corners */
        }
    </style>

